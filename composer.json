{
    "name": "tal7aouy/sslchecker",
    "description": "The SSLChecker library is a PHP class that allows you to check the expiry date of an SSL certificate for a given domain",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Tal7aouy\\SSLChecker\\": "src/"
        }
    },
    "authors": [
        {
            "name": "M-Talhaouy",
            "email": "tal7aouy.dev@gmail.com",
            "role": "PHP Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "tal7aouy/dd": "^1.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.2"
    },
    "autoload-dev": {
    "psr-4": {
      "Tal7aouy\\SSLChecker\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/pest"
    ]
  },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
